{
  "name": "@adminjs/typeorm",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "version": "4.0.0",
  "description": "TypeORM adapter for AdminJS",
  "keywords": [
    "typeorm",
    "provider",
    "adminjs",
    "orm admin",
    "typeorm admin",
    "admin panel"
  ],
  "scripts": {
    "clean": "rm -fR lib",
    "build": "tsc",
    "dev": "yarn clean && tsc -w",
    "test": "mocha --loader=ts-node/esm ./spec/**/*.spec.ts",
    "ts-node": "ts-node",
    "lint": "eslint './src/**/*.{ts,js}' './spec/**/*.{ts,js}' './example-app/**/*.{ts,js}' --ignore-pattern 'build' --ignore-pattern 'yarn.lock'",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run -d spec/utils/test-data-source.ts",
    "check:all": "yarn lint && yarn test && yarn build",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoftwareBrothers/adminjs-typeorm.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Artem Zabolotnyi <1arteha1@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "adminjs": ">=6.0.0",
    "typeorm": "~0.3.0"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/git": "^9.0.0",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "12.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "adminjs": "^6.0.0",
    "chai": "^4.2.0",
    "class-validator": "^0.11.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "mocha": "^10.2.0",
    "pg": "^8.7.3",
    "semantic-release": "^17.0.7",
    "semantic-release-slack-bot": "^1.6.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typeorm": "^0.3.12",
    "typescript": "^4.9.5"
  }
}
